<form method="POST" action="/students">

  <label for="first_name_field">First name:</label>
  <input id="first_name_field" name="first_name" type="text">

  <label for="last_name_field">Last name:</label>
  <input id="last_name_field" name="last_name" type="text">

  <label for="age">Age:</label>
  <input id="age" name="age" type="number">

  <label for="house-select">Select student's house:</label>
  <select id="house-select" name="house_id">
    <option value=1>Gryffindor</option>
    <option value=2>Ravenclaw</option>
    <option value=3>Hufflepuff</option>
    <option value=4>Slytherin</option>
  </select>

  <input type="submit" value="Create student">

</form>